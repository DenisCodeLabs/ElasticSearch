# Creamos el indice 

DELETE /i_ktra_employee

PUT /i_ktra_employee
{
  "mappings": {
    "properties": {
      "gf_company_name": {
        "type": "keyword"
      },
      "gf_date_admission": {
        "type": "date",
        "format": "epoch_millis||yyyy-MM-dd||yyyy-MM-dd HH:mm:ss||yyyy-MM-dd HH:mm:ss.SSS||yyyy-MM-dd HH:mm:ss.SSSZ||yyyyMMdd'T'HHmmss.SSSZ"
      },
      "gf_salary": {
        "type": "double"
      },
      "gf_email": {
        "type": "text"
      },
      "gf_address": {
        "type": "text"
      },
      "gf_cell_phone_number": {
        "type": "keyword"
      },
      "gf_company_cell": {
        "type": "keyword"
      }
    }
  }
}

